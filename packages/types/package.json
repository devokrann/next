{
  "name": "@repo/types",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "check-types": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./models": {
      "types": "./src/models/index.ts",
      "import": "./dist/models/index.js",
      "default": "./dist/models/index.js"
    }
  },
  "devDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@repo/enums": "^1.0.0"
  }
}
